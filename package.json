{
  "private": true,
  "name": "krantz.app",
  "description": "Personal website built with React/Remix",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "node -r dotenv/config ./node_modules/@remix-run/dev/cli.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@supabase/supabase-js": "^1.29.1",
    "framer-motion": "^5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "sideEffects": false
}
